<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shoravsuriyal.image_classifier3">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_retino"
        android:label="Retino"
        android:roundIcon="@mipmap/icon_retino_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.openpower.pes_img_classifier.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.openpower.pes_img_classifier.Login">
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.openpower.pes_img_classifier.MainActivity"
            android:label="Retino"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.openpower.pes_img_classifier.Register"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


        <activity android:name="com.openpower.pes_img_classifier.OptionScreen" />
        <activity android:name="com.openpower.pes_img_classifier.ReportActivity"></activity>
    </application>

</manifest>